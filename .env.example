# Base44 / PropAI
PROPAI_SEND_URL=https://liveispropai.base44.app/api/apps/690cfb8070b3f94428fee21c/functions/sendToPropAI
PROPAI_API_KEY=replace-with-your-api-key

# Optional: if you want to call parse functions directly
PROPAI_PARSE_PROPERTY_URL=
PROPAI_PARSE_REQUIREMENT_URL=

# WhatsApp (WPPConnect)
WPP_SESSION_NAME=real-estate-agent

# Behavior
DEDUPE_TTL_MS=600000
LOG_LEVEL=info

# Agentic App Server
PORT=8080
DATABASE_URL=
PROPAI_LIVE_POST_URL=
PROPAI_LIVE_99ACRES_POST_URL=
PROPAI_LIVE_MAGICBRICKS_POST_URL=
PROPAI_LIVE_API_KEY=
PROPAI_LIVE_TIMEOUT_MS=8000
PROPAI_LIVE_MAX_RETRIES=2
PROPAI_LIVE_RETRY_BACKOFF_MS=300
AGENT_API_KEY=
AGENT_ALLOWED_ROLES=realtor_admin,ops
CORS_ORIGIN=*
AGENT_RATE_LIMIT_WINDOW_MS=60000
AGENT_RATE_LIMIT_MAX=180
AGENT_MAX_BODY_BYTES=1048576
SKILLS_PIPELINE_ENABLED=true
PROPAI_QUEUE_ENABLED=false
PROPAI_QUEUE_NAME=propai-session-execution
PROPAI_QUEUE_ATTEMPTS=3
PROPAI_QUEUE_BACKOFF_MS=1000
PROPAI_QUEUE_CONCURRENCY=2
PROPAI_QUEUE_TIMEOUT_MS=45000
GROUP_POSTING_ENABLED=false
GROUP_POSTING_INTERVAL_MS=900000
GROUP_POSTING_BATCH_SIZE=10
GROUP_POSTING_PROCESSING_LEASE_MS=600000
GROUP_POSTING_DEFAULT_TARGETS=
GROUP_POSTING_SCHEDULER_DRY_RUN=true
GROUP_POSTING_INTAKE_ENABLED=false
GROUP_POSTING_INPUT_CHATS=
GROUP_POSTING_ACK_INPUT=false

# OpenRouter (backend /agent/chat + parser fallback + CLI)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_TIMEOUT_MS=30000
OPENROUTER_APP_NAME=PropAI Live
OPENROUTER_APP_URL=https://propai.live

# Local Ollama fallback (used when OpenRouter is unavailable)
# auto | true | false
OLLAMA_ENABLED=auto
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=llama3.1:8b
OLLAMA_TIMEOUT_MS=12000

# OpenClaw gateway bridge (used by `propai doctor`)
OPENCLAW_GATEWAY_HTTP_URL=http://127.0.0.1:19001
OPENCLAW_GATEWAY_WS_URL=ws://127.0.0.1:19001
OPENCLAW_GATEWAY_TIMEOUT_MS=3500
OPENCLAW_GATEWAY_API_KEY=

# wacli integration
# Keep true for local testing. Set to false once wacli is installed and authenticated.
WACLI_DRY_RUN=true
WACLI_BIN=wacli

# Single-agent WhatsApp helper policy
# pairing | allowlist | open | disabled
# pairing requires DATABASE_URL (persistent approvals)
WHATSAPP_DM_POLICY=allowlist
# Comma-separated E.164 numbers (e.g. +919999999999,+14155550123)
WHATSAPP_ALLOW_FROM=

# Optional Cloud API webhook verification/signature hardening
WHATSAPP_WEBHOOK_VERIFY_TOKEN=
WHATSAPP_APP_SECRET=
