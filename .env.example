# Local Ollama fallback (used when OpenRouter is unavailable)
# auto | true | false
OLLAMA_ENABLED=auto
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=llama3.1:8b
OLLAMA_TIMEOUT_MS=12000

# OpenClaw gateway bridge (used by `propai doctor`)
OPENCLAW_GATEWAY_HTTP_URL=http://127.0.0.1:19001
OPENCLAW_GATEWAY_WS_URL=ws://127.0.0.1:19001
OPENCLAW_GATEWAY_TIMEOUT_MS=3500
