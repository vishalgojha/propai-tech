{
  "name": "propai-tech",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "propai": "dist/cli/propai.js"
  },
  "scripts": {
    "dev": "tsx src/agentic-app.ts",
    "dev:legacy": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node dist/tests/run-tests.js",
    "release": "node scripts/release.mjs",
    "release:check": "node scripts/release.mjs --check",
    "release:dry": "node scripts/release.mjs --dry-run",
    "start": "node dist/agentic-app.js",
    "start:legacy": "node dist/index.js",
    "openrouter:chat": "tsx src/cli/openrouter-chat.ts",
    "propai": "tsx src/cli/propai.ts",
    "install:oneclick": "powershell -NoProfile -ExecutionPolicy Bypass -File ./install/install-propai.ps1 -FromSource",
    "terminal": "tsx src/cli/propai-termui.ts",
    "terminal:classic": "tsx src/cli/interactive-terminal.ts",
    "terminal:menu": "tsx src/cli/interactive-terminal.ts --menu",
    "terminal:tui": "tsx src/cli/propai-termui.ts",
    "web:install": "npm install --prefix web",
    "web:dev": "npm run dev --prefix web",
    "web:build": "npm run build --prefix web",
    "web:preview": "npm run preview --prefix web"
  },
  "dependencies": {
    "@vue-termui/core": "^0.2.0",
    "@wppconnect-team/wppconnect": "^1.31.0",
    "dotenv": "^16.4.5",
    "pg": "^8.18.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/pg": "^8.16.0",
    "tsx": "^4.7.3",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20 <23",
    "npm": ">=10 <12"
  },
  "packageManager": "npm@10"
}
