{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ParsedMessageInput",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "sender": {
        "type": "string",
        "minLength": 1
      },
      "content": {
        "type": "string"
      }
    },
    "required": [
      "timestamp",
      "sender",
      "content"
    ],
    "additionalProperties": false
  }
}
