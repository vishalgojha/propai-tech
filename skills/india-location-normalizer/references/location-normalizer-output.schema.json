{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "IndiaLocationNormalizerOutput",
  "type": "object",
  "properties": {
    "normalized_locations": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "lead_id": {
            "type": "string",
            "minLength": 1
          },
          "city": {
            "type": "string",
            "enum": [
              "Mumbai",
              "Pune",
              "Unknown"
            ]
          },
          "locality_canonical": {
            "type": "string",
            "minLength": 1
          },
          "micro_market": {
            "type": "string",
            "minLength": 1
          },
          "matched_alias": {
            "type": "string",
            "minLength": 1
          },
          "confidence": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
          },
          "unresolved_flag": {
            "type": "boolean"
          },
          "resolution_method": {
            "type": "string",
            "enum": [
              "exact_alias",
              "normalized_alias",
              "fuzzy_alias",
              "unresolved"
            ]
          }
        },
        "required": [
          "lead_id",
          "city",
          "locality_canonical",
          "micro_market",
          "matched_alias",
          "confidence",
          "unresolved_flag",
          "resolution_method"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "normalized_locations"
  ],
  "additionalProperties": false
}
