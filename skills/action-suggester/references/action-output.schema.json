{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ActionSuggesterOutput",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "action_type": {
        "type": "string",
        "enum": [
          "call",
          "email",
          "visit"
        ]
      },
      "lead_id": {
        "type": "string",
        "minLength": 1
      },
      "description": {
        "type": "string",
        "minLength": 1
      }
    },
    "required": [
      "action_type",
      "lead_id",
      "description"
    ],
    "additionalProperties": false
  }
}
